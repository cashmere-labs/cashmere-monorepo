{
    "name": "@cashmere-monorepo/shared-blockchain",
    "version": "0.0.0",
    "private": true,
    "files": [
        "dist"
    ],
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "clean": "rimraf dist *.tsbuildinfo",
        "test": "sst bind vitest run",
        "test:watch": "sst bind vitest run",
        "typecheck": "tsc -noEmit",
        "package": "pnpm clean && pnpm package:transpile && pnpm package:types && pnpm package:types-aliases",
        "package:transpile": "tsup",
        "package:types": "tsc -p tsconfig.build.json",
        "package:types-aliases": "tsc-alias -p tsconfig.build.json"
    },
    "devDependencies": {
        "@types/node": "^18.16.6",
        "rimraf": "^5.0.0",
        "sst": "^2.8.11",
        "tsc-alias": "^1.8.6",
        "tsup": "^6.7.0",
        "vitest": "^0.31.0"
    },
    "dependencies": {
        "viem": "^0.3.18"
    }
}
