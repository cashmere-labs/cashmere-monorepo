{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "package"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^package",
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "package": {
      "dependsOn": [
        "^package"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  },
  "defaultBase": "main"
}
