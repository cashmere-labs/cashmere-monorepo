{
  "name": "@cashmere-monorepo/backend-blockchain",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "test": "sst bind vitest",
    "typecheck": "tsc -noEmit",
    "package": "pnpm clean && pnpm package:transpile && pnpm package:types && pnpm package:types-aliases",
    "package:transpile": "tsup",
    "package:types": "tsc -p tsconfig.build.json",
    "package:types-aliases": "tsc-alias -p tsconfig.build.json"
  },
  "dependencies": {
    "@cashmere-monorepo/backend-core": "workspace:^"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.115",
    "@types/node": "^18.16.5",
    "rimraf": "^5.0.0",
    "sst": "^2.8.5",
    "vitest": "^0.31.0",
    "tsc-alias": "^1.8.6",
    "tsup": "^6.7.0"
  }
}